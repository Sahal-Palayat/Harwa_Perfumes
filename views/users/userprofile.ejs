                                                                   <%} else{%>
                                                                    <td><a class="btn-small d-block" style="color: red;">Cancel</a></td>
                                                                    <%}%>
                                                                    <td><a href="/orderView?id=<%- order._id%>" class="btn-small d-block">View</a></td>
                                                                </tr>

                                                         <%   }) %>
                                                            
                                                           
                                                           
